{
  "name": "@tinkoff/eslint-config-angular",
  "version": "1.0.0-rc.7",
  "description": "Tinkoff ESLint configs for Angular apps",
  "license": "Apache-2.0",
  "keywords": [
    "eslint",
    "eslintconfig",
