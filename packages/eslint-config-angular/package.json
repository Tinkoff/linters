    "@tinkoff/eslint-config": "^1.0.0-rc.7"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "eslint": "7.2.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-sort-class-members": "^1.7.0"
  },
  "publishConfig": {
    "access": "public"
